{
  "manifest_version": 2,
  "name": "Twitter share",
  "version": "0.0",
  "description": "Tweet any text from any webpage without going to twitter",
  "icons": {
        "16":"images/twitter_16.png",
        "32":"images/twitter_32.png",
        "64":"images/twitter_64.png",
        "128":"images/twitter_128.png"
  },
  "background":{
        "scripts": ["background.js"]
    },
  "permissions": ["contextMenus"]
}